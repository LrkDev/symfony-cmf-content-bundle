{
    "name": "symfony-cmf/content-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Content Bundle",
    "keywords": [
        "Symfony CMF"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/ContentBundle/contributors"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:lrkdev/symfony-cmf-core-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:lrkdev/symfony-cmf-menu-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:lrkdev/symfony-cmf-testing.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/framework-bundle": "^6.3",
        "symfony/templating": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/validator": "^6.3",
        "symfony-cmf/core-bundle": "dev-master",
        "symfony-cmf/menu-bundle": "dev-master",
        "symfony-cmf/routing-bundle": "^3.0"
    },
    "require-dev": {
        "doctrine/phpcr-odm": "^1.7",
        "symfony-cmf/testing": "dev-master",
        "symfony/phpunit-bridge": "^6.3",
        "jackalope/jackalope-jackrabbit": "^1.4"
    },
    "suggest": {
        "friendsofsymfony/rest-bundle": "Improved handling for different output formats",
        "doctrine/phpcr-odm": "To persist content with the PHP content repository",
        "doctrine/phpcr-bundle": "To integrate PHPCR-ODM with Symfony",
        "symfony-cmf/sonata-admin-integration-bundle": "To provide admin interfaces for the content"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\ContentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\ContentBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
